   "source": [
    "CREATE OR REPLACE FUNCTION calculate_percent_profit_loss(curr FLOAT, prev FLOAT) \n",
    "RETURNS FLOAT \n",
    "LANGUAGE SQL \n",
    "AS \n",
    "$$\n",
    "    CASE \n",
    "        WHEN prev = 0 THEN 100\n",
    "        ELSE trunc(((curr - prev) / prev) * 100,2)\n",
    "    END\n",
    "$$;"
   ]
  }
